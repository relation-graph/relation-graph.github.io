{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "^\\/$",
      "destination": "/index.html"
    },
    {
      "source": "^\\/(.*)\\/assets.*$",
      "destination": "/$1/assets/:splat"
    },
    {
      "source": "^\\/(.*)$",
      "destination": "/$1"
    }
  ]
}
